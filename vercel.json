{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "status": 301,
      "headers": { "Location": "/registration" }
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
